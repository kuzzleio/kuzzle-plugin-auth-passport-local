{
  "name": "kuzzle-plugin-auth-passport-local",
  "version": "3.0.4",
  "description": "Kuzzle plugin to log-in users",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuzzleio/kuzzle-plugin-auth-passport-local.git"
  },
  "scripts": {
    "test": "npm run --silent lint",
    "lint": "eslint --max-warnings=0 ./lib"
  },
  "keywords": [
    "kuzzle",
    "plugin",
    "auth",
    "passportjs"
  ],
  "author": {
    "name": "Kuzzle Team"
  },
  "bugs": {
    "url": "https://github.com/kuzzleio/kuzzle-plugin-auth-passport-local/issues"
  },
  "pluginInfo": {
    "defaultConfig": {
      "secret": "changeme",
      "algorithm": "sha1",
      "digest": "hex"
    }
  },
  "license": "Apache-2.0",
  "dependencies": {
    "lodash.isstring": "^4.0.1",
    "passport": "0.3.2",
    "passport-local": "1.0.0"
  },
  "devDependencies": {
    "eslint": "1.9.0"
  },
  "homepage": "https://github.com/kuzzleio/kuzzle-plugin-auth-passport-local#readme",
  "maintainers": [
    {
      "name": "kuzzle",
      "email": "support@kuzzle.io"
    }
  ]
}
